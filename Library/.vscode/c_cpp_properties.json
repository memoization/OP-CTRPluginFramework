{
    "env": {
        "devkitpro": "/opt/devkitpro",
        "devkitarm": "${devkitpro}/devkitARM",
        "ctrulib": "${devkitpro}/libctru"
    },
    "configurations": [
        {
            "name": "Library - Unix",
            "includePath": [
                "${workspaceFolder}/include/**",
                "${ctrulib}/include/**",
                "C:/devkitPro/libctru/include"
            ],
            "defines": [
                "ARM11",
                "_3DS"
            ],
            "browse": {
                "path": [
                    "${workspaceFolder}/include/**",
                    "${ctrulib}/include/**"
                ],
                "limitSymbolsToIncludedHeaders": true,
                "databaseFilename": ""
            },
            "cStandard": "c11",
            "cppStandard": "c++11",
            "compilerPath": "${devkitarm}/bin/arm-none-eabi-g++",
            "intelliSenseMode": "gcc-arm"
        },
        {
            "name": "Library - Win",
            "includePath": [
                "${workspaceFolder}/**",
                "C:\\devkitPro\\libctru\\include",
                "C:\\devkitPro\\msys2\\usr\\include"
            ],
            "defines": [
                "_DEBUG",
                "UNICODE",
                "_UNICODE"
            ],
            "windowsSdkVersion": "10.0.18362.0",
            "compilerPath": "C:\\devkitPro\\devkitARM\\bin\\arm-none-eabi-g++.exe",
            "cStandard": "c11",
            "cppStandard": "c++17",
            "intelliSenseMode": "gcc-arm"
        }
    ],
    "version": 4
}